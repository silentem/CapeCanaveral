<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.whaletail.capecanaveral">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
            android:allowBackup="true"
            android:banner="@drawable/ic_launcher_background"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".arcprogress.ArcProgressActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
        <activity android:name=".customcoordinator.CustomCoordinatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".coroutines.CoroutinesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".itemDecorator.ItemDecoratorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
                android:name=".connectioncheckbroadcast.ConnectionReceiver"
                android:enabled="true">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>

        <activity android:name=".connectioncheckbroadcast.ConnectionCheckBroadcastActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.windowsoftinputmode.WindowSoftInputModeActivity"
                android:label="@string/title_activity_window_soft_input_mode"
                android:theme="@style/AppTheme.NoActionBar"
                android:windowSoftInputMode="adjustPan|stateVisible">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.relinquishactivity.RelinquishActivityActivity"
                android:label="@string/title_activity_relinquish_activity"
                android:launchMode="singleTask"
                android:relinquishTaskIdentity="true"
                android:taskAffinity=""
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                        android:host="sportbank.com.ua"
                        android:scheme="https" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.persistablemode.PersistableModeActivity"
                android:label="@string/title_activity_persistable_mode"
                android:persistableMode="persistAcrossReboots"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.parentactivityname.ParentActivityNameActivity"
                android:label="@string/title_activity_parent_name"
                android:parentActivityName=".activityMainfestTags.documentlaunchmode.DocumentLaunchModeActivity"
                android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".activityMainfestTags.launchmode.LaunchModeStandartActivity" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.documentlaunchmode.DocumentLaunchModeActivity"
                android:documentLaunchMode="never"
                android:label="@string/title_activity_document_launch_mode"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.configchanges.ConfigChangesActivity"
                android:configChanges="orientation|keyboardHidden|locale"
                android:label="@string/title_activity_config_changes"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.launchmode.ReparentingActivity"
                android:allowTaskReparenting="true"
                android:taskAffinity="different.taskContainer"></activity>
        <activity
                android:name=".activityMainfestTags.launchmode.LaunchModeStandartActivity"
                android:label="@string/title_activity_launch_mode_standart"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.launchmode.LaunchModeSingleTaskActivity"
                android:label="@string/title_activity_launch_mode_single_task"
                android:launchMode="singleTask"
                android:taskAffinity="different.taskContainer"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.launchmode.LaunchModeSingleTopActivity"
                android:label="@string/title_activity_launch_mode"
                android:launchMode="singleTop"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".activityMainfestTags.allowtaskreparenting.AllowTaskReparentingActivity"
                android:label="@string/title_activity_allow_task_reparenting"
                android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
                android:name=".activityMainfestTags.allowEmbeded.BubbleAllowEmbededActivity"
                android:allowEmbedded="true"
                android:label="@string/title_activity_bubble_allow_embeded"></activity>
        <activity
                android:name=".activityMainfestTags.allowEmbeded.AllowEmbedActivity"
                android:label="@string/title_activity_allow_embed"></activity>
        <activity android:name=".gitRebase.GitRebaseActivity"></activity>
        <activity android:name=".customView.CustomTestViewActivity"></activity>
        <activity android:name=".rx.RxCh1Activity"></activity>
        <activity android:name=".rx.ReplaySubjectExampleActivity"></activity>

        <service
                android:name=".count.ObservableService"
                android:enabled="true"
                android:exported="true"></service>

        <activity android:name=".count.CountActivity"></activity>
    </application>

</manifest>